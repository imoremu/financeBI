			    ROUND (
			        CALCULATE (
			            SUM ( Transacciones[Participaciones] ),
			            Transacciones[Tipo Transacción] <> "Dividendos",
			            Calendario[Date] <= ultima_fecha
			        ),
			        3
			    )
